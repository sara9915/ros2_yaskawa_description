<?xml version='1.0' encoding='utf-8'?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="motoman_sia5f_arm">
  
	<xacro:property name="deg" value="0.017453293" /> 
  <xacro:property name="joint_damping" value="10" />

  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/visual/base.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/collision/base.dae"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="400"/>
      <!-- Arbitrarily large value, to simulate bolting down in case of absence of fixed joint -->
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>
  
  <link name="link_s">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/visual/link_s.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/collision/link_s.dae"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="13.746057"/>
      <inertia ixx="0.070027808" ixy="0.0" ixz="0.0" iyy="0.077808312" iyz="0.0" izz="0.043654657"/>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0046451930 -0.018976267"/>
    </inertial>
  </link>
  
  <joint name="joint_s" type="revolute">
    <origin xyz="0 0 0.31" rpy="0 0 0" /> 
    <parent link="base_link" />
    <child link="link_s" />
    <axis xyz="0 0 1" />
    <limit lower="${-180*deg}" upper="${180*deg}" effort="0" velocity="${200*deg}" />
    <dynamics damping="${joint_damping}" />
  </joint>

  <link name="link_l">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/visual/link_l.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/collision/link_l.dae"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="5.227999"/>
      <inertia ixx="0.016063413" ixy="0.0" ixz="0.0" iyy="0.10522920" iyz="0.0" izz="0.10250891"/>
      <origin rpy="0.0 0.0 0.0" xyz="0.11422130 0.0 0.018935113"/>
    </inertial>
  </link>
  
  <joint name="joint_l" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="link_s" />
    <child link="link_l" />
    <axis xyz="0 1 0" />
    <limit lower="${-110*deg}" upper="${110*deg}" effort="0" velocity="${200*deg}" />
    <dynamics damping="${joint_damping}" />
  </joint>

  <link name="link_e">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/visual/link_e.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/collision/link_e.dae"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="4.7092004"/>
      <inertia ixx="0.030190273" ixy="0.0" ixz="0.0" iyy="0.030943336" iyz="0.0" izz="0.0096760191"/>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 -0.011901001 -0.036023969"/>
    </inertial>
  </link>
  
  <joint name="joint_e" type="revolute">
    <origin xyz="0 0 0.27" rpy="0 0 0" />
    <parent link="link_l" />
    <child link="link_e" />
    <axis xyz="0 0 1" />
    <limit lower="${-170*deg}" upper="${170*deg}" effort="0" velocity="${200*deg}" />
    <dynamics damping="${joint_damping}" />
  </joint>
  
  <link name="link_u">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/visual/link_u.dae"/>
      </geometry>
    </visual>
    <collision>axis
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/collision/link_u.dae"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="3.1488649"/>
      <inertia ixx="0.083840132" ixy="0.0" ixz="0.0" iyy="0.0061641860" iyz="0.0" izz="0.083291837"/>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 -0.015659426 0.13525753"/>
    </inertial>
  </link>
  
  <joint name="joint_u" type="revolute">
    <origin xyz="0.085 0 0" rpy="0 0 0" />
    <parent link="link_e" />
    <child link="link_u" />
    <axis xyz="0 -1 0" />
    <limit lower="${-90*deg}" upper="${115*deg}" effort="0"  velocity="${200*deg}" />
    <dynamics damping="${joint_damping}" />
  </joint>

  <link name="link_r">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/visual/link_r.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/collision/link_r.dae"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="3.0270943"/>
      <inertia ixx="0.017471059" ixy="0.0" ixz="0.0" iyy="0.017175292" iyz="0.0" izz="0.0049008824"/>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 -0.011271807 -0.0416"/>
    </inertial>
  </link>
  
  <joint name="joint_r" type="revolute">
    <origin xyz="0.27 0 0.06" rpy="0 0 0" />
    <parent link="link_u" />
    <child link="link_r" />
    <axis xyz="-1 0 0" />
    <limit lower="${-90*deg}" upper="${90*deg}" effort="0" velocity="${200*deg}" />
    <dynamics damping="${joint_damping}" />
  </joint>

  <link name="link_b">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/visual/link_b.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/collision/link_b.dae"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.3902999"/>
      <inertia ixx="0.080" ixy="0.0" ixz="0.0" iyy="0.0038377449" iyz="0.0" izz="0.080"/>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0074364051 0.078721289"/>
    </inertial>
  </link>
  
  <joint name="joint_b" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="link_r" />
    <child link="link_b" />
    <axis xyz="0 -1 0" />
    <limit lower="${-110*deg}" upper="${110*deg}" effort="0" velocity="${230*deg}" />
    <dynamics damping="${joint_damping}" />
  </joint>

  <link name="link_t">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/visual/link_t.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find yaskawa_description)/meshes/collision/link_t.dae"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.63670412"/>
      <inertia ixx="0.00058471221" ixy="0.0" ixz="0.0" iyy="0.0005929671" iyz="0.0" izz="0.001081389"/>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
    </inertial>
  </link>
  
  <joint name="joint_t" type="revolute">
    <origin xyz="0.134 -0.0078 0" rpy="0 0 0" />
    <parent link="link_b" />
    <child link="link_t" />
    <axis xyz="-1 0 0" />
    <limit lower="${-90*deg}" upper="${90*deg}" effort="0" velocity="${350*deg}" />
    <dynamics damping="${joint_damping}" />
  </joint>
  
  <link name="tool0" />
  
  <joint name="link_t-tool0" type="fixed" >
    <origin xyz="0.011 0 0" rpy="${-90*deg} ${-90*deg} ${-90*deg}"/>
    <parent link="link_t" />
    <child link="tool0" />
    <!-- <dynamics damping="${joint_damping}" /> -->
  </joint> 
  


</robot>
